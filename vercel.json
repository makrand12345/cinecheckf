{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/browser",
  "routes": [
    {
      "src": "/main.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/runtime.js", 
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}